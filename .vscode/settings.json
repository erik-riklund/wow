{
  "files.exclude": {
    "**/**.zip": true,
    "!KalielsTracker/**": true,
    "Auctionator/**": true,
    "Baganator/**": true,
    "Blizz*/**": true,
    "Details*/**": true,
    "EasyDelete/**": true,
    "ElvUI*/**": true,
    "GatherMate2/**": true,
    "Immersion/**": true,
    "Leatrix_Plus/**": true,
    "ls_*/**": true,
    "Masque*/**": true,
    "Mouseover*/**": true,
    "MoveAny/**": true,
    "NameplateSCT/**": true,
    "Plater/**": true,
    "Pretty*/**": true,
    "Quartz/**": true,
    "Syndicator/**": true,
    "TooltipCleaner/**": true,
  },
  "Lua.diagnostics.disable": [
    "trailing-space",
    "empty-block",
    "unused-function"
  ],
  "better-comments.multilineComments": true,
  "better-comments.tags": [
    {
      "tag": "[[~",
      "color": "#D5AA4D",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
  ],
  "Lua.runtime.version": "Lua 5.1",
  "stylua.targetReleaseVersion": "latest"
}