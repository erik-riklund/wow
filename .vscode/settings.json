{
  "files.exclude": {
    "**/**.zip": true
  },
  "Lua.diagnostics.disable": [
    "trailing-space",
    "empty-block",
    "unused-function"
  ],
  "better-comments.multilineComments": true,
  "better-comments.tags": [
    {
      "tag": "[[~",
      "color": "#D5AA4D",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
  ],
  "Lua.runtime.version": "Lua 5.1",
  "Lua.runtime.builtin": {
    "basic": "disable",
    "debug": "disable",
    "io": "disable",
    "math": "disable",
    "os": "disable",
    "package": "disable",
    "string": "disable",
    "table": "disable",
    "utf8": "disable"
  },
  "Lua.workspace.library": [
    "~\\.vscode\\extensions\\ketho.wow-api-0.17.6\\Annotations"
  ]
}